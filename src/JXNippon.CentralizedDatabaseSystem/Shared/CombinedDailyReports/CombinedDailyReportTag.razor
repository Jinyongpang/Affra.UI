
@if (this.GetTotalCount() > 0)
{
    
    <Tag Color="@(this.GetColor())" Class="panel-summary-tag">@this.TotalCount</Tag>

}
else if(this.HasEmptyRowData)
{
    <Tag Color="@(this.GetColor())" Class="panel-summary-tag">No Data</Tag>
}
else
{
    <Tag Color="@(this.GetColor())" Class="panel-summary-tag">@this.TotalCount</Tag>
}