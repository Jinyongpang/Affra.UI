                    </div>
                    <div class="row py-2">
                        <div class="col-4 align-items-center d-flex">
                            <RadzenLabel class="dialogTextSize" Text="DE Journal Brg TE-808 °c" />
                        </div>
                        <div class="col-8">
                            <RadzenNumeric Name="DEJournalBrgTE808" class="w-100" @bind-Value="@Item.DEJournalBrgTE808" Disabled=@isViewing Placeholder="Enter DE journal brg TE-808 °c ..." TextProperty="DEJournalBrgTE808"></RadzenNumeric>
