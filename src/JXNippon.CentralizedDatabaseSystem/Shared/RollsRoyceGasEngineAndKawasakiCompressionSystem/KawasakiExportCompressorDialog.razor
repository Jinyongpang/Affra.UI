                        </div>
                        <div class="col-8">
                            <RadzenNumeric Name="DEJournalBrgTE807" class="w-100" @bind-Value="@Item.DEJournalBrgTE807" Disabled=@isViewing Placeholder="Enter DE journal brg TE-807 °c ..." TextProperty="DEJournalBrgTE807"></RadzenNumeric>
