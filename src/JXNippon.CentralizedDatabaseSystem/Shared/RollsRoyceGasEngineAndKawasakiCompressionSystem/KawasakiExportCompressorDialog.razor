                            <RadzenAutoComplete Name="KawasakiExportCompressorName" class="w-100" @bind-Value="@Item.KawasakiExportCompressorName" Disabled=@isViewing FilterCaseSensitivity="FilterCaseSensitivity.CaseInsensitive"
