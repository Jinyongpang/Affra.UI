                            <RadzenNumeric Name="PressureSuct" class="w-100" @bind-Value="@Item.PressureSuct" Disabled=@isViewing Placeholder="Enter pressure suct ..." TextProperty="PressureSuct"></RadzenNumeric>
                        </div>
                    </div>
                    <div class="row py-2">
                        <div class="col-4 align-items-center d-flex">
                            <RadzenLabel class="dialogTextSize" Text="Pressure (bar) Disc (HP)" />
                        </div>
                        <div class="col-8">
                            <RadzenNumeric Name="PressureDisc" class="w-100" @bind-Value="@Item.PressureDisc" Disabled=@isViewing Placeholder="Enter pressure disc ..." TextProperty="PressureDisc"></RadzenNumeric>
                        </div>
                    </div>
                    <div class="row py-2">
                        <div class="col-4 align-items-center d-flex">
                            <RadzenLabel class="dialogTextSize" Text="Throughput MMscfd" />
